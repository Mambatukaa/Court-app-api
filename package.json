{
  "name": "mamba-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "keywords": [
    "node",
    "express",
    "graphql",
    "apollo"
  ],
  "scripts": {
    "start": "node dist",
    "dev": "NODE_ENV=development nodemon src --exec babel-node",
    "build": "babel src --out-dir dist --ignore __tests__,tests --copy-files",
    "lint": "eslint src",
    "format": "prettier --write --print-width 100 --single-quote --trailing-comma all 'src/**/*.js'",
    "precommit": "lint-staged",
    "test": "jest --runInBand --coverage",
    "loadInitialUser": "babel-node ./src/initialUser.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --print-width 100 --single-quote --trailing-comma all",
      "git add"
    ]
  },
  "dependencies": {
    "aws-sdk": "^2.372.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "formidable": "^1.1.1",
    "graphql": "^15.4.0",
    "graphql-server-core": "^1.4.1",
    "graphql-server-express": "^1.4.1",
    "graphql-server-module-graphiql": "^1.4.1",
    "graphql-tools": "^7.0.2",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.18.1",
    "mongoose": "4.13.14",
    "mongoose-type-email": "^1.0.5",
    "nodemailer": "^4.7.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nodemailer-sendinblue-transport": "^1.2.3",
    "sha256": "^0.2.0",
    "xlsx-populate": "^1.19.1"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "faker": "^4.1.0",
    "husky": "^0.13.4",
    "jest": "^21.2.1",
    "jest-tobetype": "^1.1.0",
    "lint-staged": "^3.6.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.4.4"
  }
}
